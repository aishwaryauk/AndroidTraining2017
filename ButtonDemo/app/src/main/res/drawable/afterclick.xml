<?xml version="1.0" encoding="utf-8"?>
<shape android:shape="rectangle"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <corners android:bottomLeftRadius="80dp"
        android:bottomRightRadius="25dp"
        android:topRightRadius="25dp"
        android:topLeftRadius="80dp"></corners>

    <size android:height="100dp" android:width="100dp"></size>

    <gradient
        android:centerColor="#ffffff"
        android:startColor="#e68d08"
        android:endColor="#0fab3b"
        android:angle="270"
        android:gradientRadius="15dp">

    </gradient>

    <stroke
        android:color="#000000"
        android:dashGap="5dp"
        android:width="1dp"
        android:dashWidth="15dp"></stroke>




</shape>